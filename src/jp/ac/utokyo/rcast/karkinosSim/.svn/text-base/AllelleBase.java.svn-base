package jp.ac.utokyo.rcast.karkinosSim;

public class AllelleBase {
	
	String alleleA="";
	String alleleB="";
	public String getAlleleA() {
		return alleleA;
	}
	public void setAlleleA(String allelA) {
		this.alleleA = allelA;
	}
	public String getAlleleB() {
		return alleleB;
	}
	public void setAlleleB(String alleleB) {
		this.alleleB = alleleB;
	}
	public boolean indel(){
		return alleleA.length()>2;
	}

}
